#include <stdio.h>
 
int main() {
 
    int A, B , C, D,par;
    
    scanf("%d",&A);
    scanf("%d",&B);
    scanf("%d",&C);
    scanf("%d",&D);
    par= A%2;
    if(B>C && D>A && (C+D)>(A+B)){
      if(C>=0 && D>=0){
        if(!par){
          printf("Valores aceitos\n");
        }else{
          printf("Valores nao aceitos\n");
        }
      }else{
        printf("Valores nao aceitos\n");
      }
    }else{
      printf("Valores nao aceitos\n");
    }
    return 0;
}
