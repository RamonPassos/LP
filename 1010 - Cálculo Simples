#include <stdio.h>

int main(void) {
  int cod1;
  int cod2;
  int uni1;
  int uni2;
  double valor1;
  double valor2;
  double total;
  scanf("%d %d %lf", &cod1, &uni1, &valor1);
  scanf("%d %d %lf", &cod2, &uni2, &valor2);
  total= (uni1*valor1)+(uni2*valor2);
  printf("VALOR A PAGAR: R$ %.2lf\n", total);
  return 0;
}
